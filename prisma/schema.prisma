datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Pet {
  id        String     @id @default(auto()) @map("_id") @db.ObjectId
  name      String   @unique
}

